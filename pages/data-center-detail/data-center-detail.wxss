/* pages/car.wxss */
.container {
  overflow: hidden;
}

.container-wrapper {
  height: 100%;
  width: 100%;
  background-color: #f6f6f6;
  overflow: auto;
}

.i-card {
  background-color: #fff;
  margin:  30rpx;
  /* padding: 10px 20px; */
  border-radius: 20rpx;
  box-shadow: 0 2rpx 5rpx rgb(18 18 18 / 10%);
  box-sizing: border-box;
}
.i-card-title{
  display: flex;
  align-items: center;
  background-color: #07c160;
  border-radius: 20rpx 20rpx 0 0;
  color:white;
  padding: 0 20rpx;
  height: 60rpx;
  line-height:60rpx;
  font-size: small;
}
.i-card-title image{
  margin-right: 10rpx;
  height: 40rpx;
  width: 40rpx;
}
.i-card-content{
  padding: 10rpx 20rpx;
  font-size: 28rpx;
}